    "@types/execa": "2.0.0",
    "@types/fs-extra": "9.0.13",
    "@types/inquirer": "8.2.1",
    "@types/inquirer-autocomplete-prompt": "1.3.5",
    "@types/jest": "29.2.1",
    "@types/js-yaml": "4.0.5",
    "@types/luxon": "2.3.1",
    "@types/node": "16.11.6",
    "@types/node-notifier": "8.0.2",
    "@types/npmlog": "4.1.4",
    "@types/parse-github-url": "1.0.0",
    "@types/request": "2.48.8",
    "@types/semver": "7.3.9",
    "@types/slug": "5.0.3",
    "@types/temp": "0.9.0",
    "@typescript-eslint/eslint-plugin": "5.18.0",
    "@typescript-eslint/parser": "5.18.0",
    "copyfiles": "2.4.1",
    "docsify": "4.12.2",
    "eslint": "7.32.0",
    "eslint-config-oclif": "4.0.0",
    "eslint-config-oclif-typescript": "1.0.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-security": "1.4.0",
    "husky": "7.0.4",
    "jest": "29.2.2",
    "lerna": "6.0.1",
    "lerna-update-wizard": "1.1.2",
    "node-fetch": "3.2.10",
    "nx": "15.0.10",
    "oclif": "3.4.3",
    "recursive-copy-cli": "1.0.20",
    "rimraf": "3.0.2",
    "shx": "0.3.4",
    "swagger2openapi": "7.0.8",
    "ts-jest": "29.0.3",
    "ts-node": "10.4.0",
    "typescript": "4.7.4",
    "yarn": "1.22.19",
    "zx": "7.0.8"
  },
  "engines": {
    "node": ">=16.13.0"
  },
  "name": "oclif-test",
  "private": true,
  "scripts": {
    "bootstrap": "yarn && yarn prepare && lerna bootstrap && yarn dev:setup",
    "dev:setup": "nx run-many --target=dev:setup",
    "build:all": "nx run-many --target=build --parallel=4",
    "test:all": "nx run-many --target=test --all --parallel=6",
    "lint:all": "nx run-many --target=lint --all --parallel=6",
    "affected:build": "nx affected:build",
    "affected:lint": "nx affected:lint",
    "affected:test": "nx affected:test",
    "clean": "lerna clean -y",
    "commit-version": "git add . && git commit -m \"chore(release): publish `node -p 'require(\"./lerna.json\").version'`\"",
    "nifo": "yarn clean && rimraf node_modules && lerna bootstrap && yarn build:all",
    "postversionup": "yarn run commit-version",
    "postversionup:major": "yarn run commit-version",
    "postversionup:minor": "yarn run commit-version",
    "postversionup:patch": "yarn run commit-version",
    "prepare": "husky install",
    "prepush": "yarn lint:all && yarn test:all",
    "release": "lerna publish --force-publish='*' --yes",
    "release:canary": "lerna publish --canary --preid next --dist-tag next --force-publish='*' --no-push --no-git-tag-version --yes",
    "release:alpha": "lerna publish --canary --preid alpha --dist-tag alpha --force-publish='*' --no-push --no-git-tag-version --yes",
    "versionup": "lerna version --conventional-commits --no-git-tag-version --force-publish='*' --no-push",
    "versionup:major": "lerna version major --conventional-commits --no-git-tag-version --force-publish='*' --no-push",
    "versionup:minor": "lerna version minor --conventional-commits --no-git-tag-version --force-publish='*' --no-push",
    "versionup:patch": "lerna version patch --conventional-commits --no-git-tag-version --force-publish='*' --no-push"
  },
  "version": "2.5.1",
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "dependencies": {}
}

