{
    "devDependencies": {
        "@commitlint/cli": "17.2.0",
        "@commitlint/config-conventional": "17.2.0",
        "@nrwl/eslint-plugin-nx": "15.6.2",
        "@nrwl/jest": "15.6.2",
        "@nrwl/linter": "15.6.2",
        "@nrwl/nx-cloud": "15.0.2",
        "@oclif/test": "2.2.8",
        "@openapitools/openapi-generator-cli": "2.5.2",
        "@types/archiver": "5.3.1",
        "@types/conventional-changelog-config-spec": "2.1.1",
        "@types/conventional-recommended-bump": "6.1.0",
        "@types/docker-modem": "3.0.2",
        "@types/dockerode": "3.3.9",
        "@types/execa": "2.0.0",
        "@types/fs-extra": "9.0.13",
        "@types/inquirer": "8.2.1",
        "@types/inquirer-autocomplete-prompt": "1.3.5",
        "@types/jest": "29.4.0",
        "@types/js-yaml": "4.0.5",
        "@types/luxon": "2.3.1",
        "@types/node": "16.11.6",
        "@types/node-notifier": "8.0.2",
        "@types/npmlog": "4.1.4",
        "@types/parse-github-url": "1.0.0",
        "@types/request": "2.48.8",
        "@types/semver": "7.3.9",
        "@types/slug": "5.0.3",
        "@types/temp": "0.9.0",
        "@typescript-eslint/eslint-plugin": "5.18.0",
        "@typescript-eslint/parser": "5.18.0",
        "copyfiles": "2.4.1",
        "docsify": "4.12.2",
        "eslint": "8.32.0",
        "eslint-config-prettier": "8.6.0",
        "husky": "7.0.4",
        "jest": "29.4.1",
        "lerna": "6.4.1",
        "lerna-update-wizard": "1.1.2",
        "node-fetch": "3.2.10",
        "nx": "15.6.2",
        "oclif": "3.6.1",
        "prettier": "2.8.1",
        "recursive-copy-cli": "1.0.20",
        "rimraf": "3.0.2",
        "shx": "0.3.4",
        "swagger2openapi": "7.0.8",
        "ts-jest": "29.0.5",
        "ts-node": "10.4.0",
        "typescript": "4.7.4",
        "yarn": "1.22.19",
        "zx": "7.0.8"
    },
    "engines": {
        "node": ">=16.13.0"
    },
    "name": "oclif-test",
    "private": true,
    "scripts": {
        "bootstrap": "yarn && yarn prepare && lerna bootstrap && yarn dev:setup",
        "dev:setup": "nx run-many --target=dev:setup",
        "build:all": "nx run-many --target=build --parallel=4",
        "build:docs": "nx run-many --target=build --parallel=4 && nx run-many --target=docs --parallel=4",
        "test:all": "nx run-many --target=test --all --parallel=6",
        "lint:all": "nx workspace-lint && yarn format",
        "affected:build": "nx affected:build",
        "affected:lint": "nx affected:lint",
        "affected:test": "nx affected:test",
        "clean": "lerna clean -y",
        "format": "nx run-many --target=lint --all --parallel=6 --fix",
        "commit-version": "git add . && git commit -m \"chore(release): publish `node -p 'require(\"./lerna.json\").version'`\"",
        "nifo": "yarn clean && rimraf node_modules && lerna bootstrap && yarn build:all",
        "postversionup": "yarn run commit-version",
        "postversionup:major": "yarn run commit-version",
        "postversionup:minor": "yarn run commit-version",
        "postversionup:patch": "yarn run commit-version",
        "prepare": "husky install",
        "prepush": "yarn lint:all && yarn test:all",
        "prerelease": "yarn build:all",
        "prerelease:canary": "yarn build:all",
        "prerelease:alpha": "yarn build:all",
        "release": "lerna publish --force-publish='*' --yes",
        "release:canary": "lerna publish --canary --preid next --dist-tag next --force-publish='*' --no-push --no-git-tag-version --yes",
        "release:alpha": "lerna publish --canary --preid alpha --dist-tag alpha --force-publish='*' --no-push --no-git-tag-version --yes",
        "versionup": "lerna version --conventional-commits --no-git-tag-version --force-publish='*' --no-push",
        "versionup:major": "lerna version major --conventional-commits --no-git-tag-version --force-publish='*' --no-push",
        "versionup:minor": "lerna version minor --conventional-commits --no-git-tag-version --force-publish='*' --no-push",
        "versionup:patch": "lerna version patch --conventional-commits --no-git-tag-version --force-publish='*' --no-push"
    },
    "workspaces": {
        "packages": [
            "packages/*"
        ]
    },
    "dependencies": {
        "@u4ic-test/core": "2.7.0",
        "@u4ic-test/oclif-test": "2.7.0",
        "@u4ic-test/plugin-connector": "2.7.0"
    },
    "version": "2.7.0"
}
