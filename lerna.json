{
    "npmClient": "yarn",
    "useNx": true,
    "packages": [
        "packages/*"
    ],
    "version": "2.6.0",
    "useWorkspaces": true,
    "command": {
        "publish": {
            "conventionalCommits": true,
            "verifyAccess": false,
            "graphType": "all"
        },
        "version": {
            "allowBranch": [
                "main"
            ]
        }
    }
}
