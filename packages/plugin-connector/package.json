{
  "author": "U4IC",
  "bugs": {
    "url": "https://github.com/youngcm2/oclif-test/issues"
  },
  "dependencies": {
    "@oclif/core": "1.16.4",
    "@u4ic-test/core": "1.0.0",
    "fs-extra": "10.0.1",
    "noop-stream": "1.0.0",
    "npmlog": "6.0.1",
    "semver": "7.3.6",
    "tslib": "2.3.1",
    "inquirer": "8.2.2",
    "inquirer-autocomplete-prompt": "2.0.0",
    "js-yaml": "4.1.0",
    "json-schema-faker": "0.5.0-rcv.46",
    "json-schema-typed": "7.0.3",
    "listr2": "4.0.5",
    "slug": "5.3.0",
    "snake-case": "3.0.4",
    "handlebars": "4.7.7"
  },
  "description": "Plugin for managing connectors.",
  "engines": {
    "node": ">=16.13.0"
  },
  "files": [
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json",
    "/yarn.lock",
    "lib"
  ],
  "homepage": "https://github.com/youngcm2/oclif-test#readme",
  "keywords": [
    "oclif-plugin"
  ],
  "license": "ISC",
  "main": "lib/index.js",
  "name": "@u4ic-test/plugin-connector",
  "directories": {
    "templates": "./lib/templates",
    "lib": "lib"
  },
  "oclif": {
    "commands": "./lib/commands",
    "bin": "u4ic-test-connector",
    "devPlugins": [
      "@oclif/plugin-help"
    ],
    "topics": {
      "connector": {
        "description": "Local connector commands"
      },
      "action": {
        "description": "Local connector action commands"
      },
      "asset": {
        "description": "Local connector asset commands"
      },
      "connectors": {
        "description": "Local connectors commands"
      }
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/youngcm2/oclif-test.git"
  },
  "scripts": {
    "build": "rm -rf lib && tsc && yarn copy-files",
    "lint": "eslint . --ext .ts --config .eslintrc",
    "copy-files": "recursive-copy **/templates/ lib/templates",
    "pretest": "tsc -p test --noEmit",
    "postpack": "shx rm -f oclif.manifest.json",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "version": "oclif readme",
    "test": "echo NO TESTS",
    "posttest": "yarn lint",
    "dev:setup": "npm link"
  },
  "version": "1.0.0"
}
